{
  "name": "doppler-effect",
  "version": "0.0.7",
  "description": "An interactive simulation of the Doppler Effect using SceneryStack.",
  "repository": {
    "type": "git",
    "url": "https://github.com/veillette/dopplerEffectPhET.git"
  },
  "type": "module",
  "keywords": [
    "Doppler Effect",
    "simulation",
    "SceneryStack",
    "interactive"
  ],
  "scripts": {
    "start": "npx vite",
    "build": "npx tsc && npx vite build",
    "lint": "npx eslint .",
    "format": "npx prettier --write .",
    "fix": "npx eslint --fix . && npx prettier --write .",
    "release": "npm version patch && git push && git push --tags",
    "check": "npx tsc --noEmit",
    "serve": "npx serve -s dist",
    "watch": "npx tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "scenerystack": "~1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.14.0",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.2.5",
    "vite-plugin-singlefile": "^2.2.0"
  }
}
